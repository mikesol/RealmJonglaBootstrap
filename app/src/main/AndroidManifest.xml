<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.jongla.realmbootstrap"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="com.jongla.realmbootstrap.RealmJonglaBootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.jongla.realmbootstrap.RegisterActivity"
            android:label="@string/activity_sign_in_label">
            <intent-filter android:label="@string/app_name">
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>